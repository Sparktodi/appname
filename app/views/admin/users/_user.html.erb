<tr>
  <th scope="row"><%= user.id %></th>
  <td><%= user.email %></td>
  <td><%= user.role %></td>
  <td><%=l user.created_at, :format => :long %></td>
  <td>
    <%= link_to t('btn.edit'), edit_admin_user_path(user), class: 'btn btn-secondary' %>
    <%= link_to t('btn.delete'), admin_user_path(user), class: 'btn btn-danger',
      data: {method: :delete, confirm: t('global.dialog.you_sure')} %>
  </td>
</tr>